{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": true,
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": false,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": false,
    "outDir": "./lib/",
    "noEmitHelpers": true,
    "rootDir": "./",
    "lib": [
      "es6",
      "es5"
    ]
  },
  "exclude": [
    "node_modules",
    "test"
  ]
}
